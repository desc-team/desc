{
    "name": "@desc/app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "start": "serve -s dist",
        "lint": "eslint .",
        "preview": "vite preview",
        "test": "jest --colors --runInBand",
        "test:e2e": "yarn dev"
    },
    "dependencies": {
        "materialize-css": "1.0.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-query": "3.39.2",
        "react-redux": "8.0.2",
        "react-router-dom": "6.3.0",
        "redux": "4.2.0",
        "redux-logger": "3.0.6",
        "redux-thunk": "2.4.1",
        "serve": "14.0.1"
    },
    "devDependencies": {
        "@babel/core": "7.17.9",
        "@babel/preset-env": "7.16.11",
        "@babel/preset-react": "7.16.7",
        "@testing-library/jest-dom": "5.16.4",
        "@testing-library/react": "12.1.2",
        "@testing-library/user-event": "13.5.0",
        "@vitejs/plugin-react": "1.3.1",
        "babel-jest": "27.5.1",
        "jest-environment-jsdom": "29.0.1",
        "vite": "2.9.5"
    },
    "jest": {
        "moduleNameMapper": {
            "^.+\\.css$": "<rootDir>/test/__mocks__/styleMock.js",
            "^.+\\.(png|svg)$": "<rootDir>/test/__mocks__/fileTransform.js"
        },
        "testEnvironment": "jsdom"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "engines": {
        "node": ">=14.16.0 <17.0.0",
        "yarn": ">=1.22.0"
    }
}
